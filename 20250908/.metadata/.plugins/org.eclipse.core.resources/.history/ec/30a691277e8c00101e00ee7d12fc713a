class Book {
	
	String title;
	int price;
	String name;
	
	Book( ){}
	
	Book(String title) {
		this.title = title;
	}
	
	Book(String title, int price) {
		this(title);
//		this.title = title;
		this.price = price;
	}
	
	Book(String title, int price, String name){
		this(title, price);
		this.name = name;
	}
}

public class Code112 {
	
	public static void main(String[] args) {
		Book b = new Book("Java", 25000, "송미영");
		
		System.out.println(b.title + " , " + b.price + " , " + b.name);
	}
}
