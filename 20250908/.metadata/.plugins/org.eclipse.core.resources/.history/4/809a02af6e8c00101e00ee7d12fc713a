class Car {
	String kind;
	String color;
	int year;
	
	void run() {
		System.out.print("자동차 달리기");
	}
	void speedUp() {
		System.out.print("자동차 속도 올리기");
	}
}


public class Code777 {

	public static void main(String[] args) {
		Car c1 = new Car();
		c1.kind = "Sonata";
		c1.color = "White";
		c1.year = 2012;
		
		System.out.println(c1);
		System.out.println(c1.kind);
		System.out.println(c1.color);
		System.out.println(c1.year);

	}

}
