
public class Ex00 {

	public static void main(String[] args) {
		System.out.println("Hello World");
		System.out.println("유한대학교");
		System.out.println("컴퓨터\n 소프트웨어공학");
		
/*		System.out.println("컴퓨터 소프트웨어공학");
		System.out.println("컴퓨터 소프트웨어공학");
		System.out.println("컴퓨터 소프트웨어공학");	*/
		
		int a = 10;
		int b = 5;
		int c = 3;
		
		System.out.println(a + b);
		System.out.println(a - b);
		System.out.println(a * b);
		System.out.println(a % b);
		System.out.println(a % c);
		
		System.out.println(++a + -b);
		System.out.println(a == b);
		System.out.println(a != b);
		System.out.println(a <= 10 || b < 5);
		System.out.println(a < 10 && b < 5);
		
		int hap = 0;
		
		for(int i = 1; i <= 10; i ++)
		{
			hap += i;
			System.out.println(hap);
		}
		System.out.println(hap);
		
		for(int i = 1; i <= 4; i++)
		{
			for(int j = 1; j <= i; j++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
		
		for(int i = 5; i >= 1; i--)
		{
			for(int j = 1; j <= i; j++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
		
		for(int i = 5; i >= 1; i--)
		{
			for(int j = 1; j < i; j++)
			{
				System.out.print(" ");
			}
			for(int k = 5; k >= i; k--)
			{
				System.out.print("*");
			}
			System.out.println();
		}
		
		for(int i = 4; i >= 1; i--)
		{
			for(int j = 5; j > i; j--)
			{
				System.out.print(" ");
			}
			for(int k = 1; k <= i; k++)
			{
				System.out.print("*");
			}
			System.out.println();
		}
		
		hap = 0;
		
		for(int i = 1; i <= 100; i++)
		{
			if(i % 4 == 0)
			{
				continue;
			}
			hap += i;
		}
		System.out.println(hap);
		
		hap = 0;
		int l = 1;
		while(l <= 100)
		{
			if(l % 3 == 0 && l % 5 == 0)
			{
				hap += l;
			}
			else
			{
				continue;
			}
			l++;
		}
		System.out.println(hap);
		
		for (int i = 2; i <= 10; i++) { // 1은 소수가 아님
            boolean isPrime = true;
            for (int j = 2; j <= i - 1; j++) { // 2부터 자신의 바로 앞 숫자까지
                if (i % j == 0) { // 나머지가 0이면 약수가 있음
                    isPrime = false;
                    break;
                }
            }
            if (isPrime) {
                System.out.print(i + " ");
            }
        }
	}

}
