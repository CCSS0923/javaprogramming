package a20250602;

public class Ex701 {

	public static void main(String[] args) {
		Cat1 c = new Cat1();
		
		c.breed = "야옹이";
		c.color = "흰색";
		
		System.out.println("품종은 : " + c.breed);
		System.out.println("색깔은 : " + c.color);

		c.eat();
		c.scratch();
		c.meow();
		
		c.breed = "쌉살이";
		c.color = "검정색";
		
		System.out.println("품종은 : " + c.breed);
		System.out.println("색깔은 : " + c.color);
		
	}

}
