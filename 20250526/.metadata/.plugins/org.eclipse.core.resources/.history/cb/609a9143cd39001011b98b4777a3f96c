package OOP;

public class Ex7011 {
	public static class Dog {
		String breed;
		String color;
		int age;
		double weight;
		
		void eat() {
			System.out.println("고양이 먹이를 먹다");
		}
		void scratch() {
			System.out.println("발톱으로 할퀴다");
		}
		void meow() {
			System.out.println("야옹~");
		}
	}

	public static void main(String[] args) {
		Cat catObj = new Cat();
		
		catObj.breed = "나비";
		catObj.color = "흰색";
		catObj.age = 5;
		catObj.weight = 15.7;
		
		System.out.println("품종 : " + catObj.breed);
		System.out.println("색깔 : " + catObj.color);
		System.out.println("나이 : " + catObj.age + "살");
		System.out.println("몸무게 : " + catObj.weight + "kg");
		
		catObj.eat();
		catObj.scratch();
		catObj.meow();
	}

}
