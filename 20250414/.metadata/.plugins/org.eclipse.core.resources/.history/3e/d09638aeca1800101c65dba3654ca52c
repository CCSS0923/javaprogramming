import java.util.Scanner;

public class b20250414 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		int n = s.nextInt();
		int cnt = 0;
		for(int i = 1; i * i <= number; i++) {
			if(i * i == number)
				cnt++;
			elseif(number % i == 0)
				cnt+=2;
		}
		
	}

}
