import java.util.Scanner;


public class Practice {

	public static void main(String[] args) {
		System.out.println("세가지 숫자를 입력해주세요");
		
		Scanner s = new Scanner(System.in);

		int x = s.nextInt();
		int y = s.nextInt();
		int z = s.nextInt();
		int result;
		
		if(x > y && x > z) {
			result = x;
		}
		else if(y > x && y > z) {
			result = y;
		}
		else {
			result = z;
		}
		
		System.out.println("가장 큰 숫자는" + result + "입니다.");
	}

}
