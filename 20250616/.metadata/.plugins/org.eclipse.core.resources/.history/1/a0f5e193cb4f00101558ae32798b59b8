package ssi;

public class SubKitten extends SuperCat {
	String age = "2살";
	
	void printInfo() {
		System.out.print("아기 고양이입니다 : ");
	}
	void printDetail() {
		super.printInfo();
		System.out.println("품종은 " + this.breed + ". 나이는 " + this.age);
		
		printInfo();
		System.out.println("품종은 " + this.breed + ". 나이는 " +  this.age);
	}
}
