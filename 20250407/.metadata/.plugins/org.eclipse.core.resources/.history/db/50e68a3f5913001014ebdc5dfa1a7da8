import java.util.Scanner;

public class d20250407 {

	public static void main(String[] args) {
		System.out.println("숫자를 입력해주세요");
		
		Scanner s = new Scanner(System.in);
		
		
		 int n, i, j, cnt = 0;

		 System.out.println("숫자를 입력해주세요");
		 int n = s.nextInt();

		 for (i = 1; i <= n; i++)
		 {
		     if (n % i == 0)
		    	 cnt++;
		 }
		 if (cnt == 2)
			 System.out.println("%d는 소수 입니다." + n);
		

	}

}
