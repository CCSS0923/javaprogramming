
public class b20250407 {

	public static void main(String[] args) {
		for(int i = 2; i < 10; i++) {
			System.out.println("구구단" + i + " 단");
			
			for(int j = 1; j < 10; j++) {
				System.out.println(i + " * " + j + " = " + (i*j));
			}
		}
		for(int i = 0; i <= 5; i++) {
			for(int j = 0; j < i; j++) {
				System.out.print("* ");
			}
			System.out.println();
		}
		for(int i = 1; i <= 5; i++) {
			System.out.print(" ");
			for(int j = 0; j < i; j++) {
				System.out.print(("*");
			}
			System.out.println();
		}
		

	}

}
