package pkg5.copy;

class Animal1 { 
	 
    public void displayInfo() { 
       System.out.println("나는 동물입니다");
    } 
    
    public void printSound() {
        System.out.println("모든 동물은 이동을 합니다");
    }
    
}

public class Ex01 {

	public static void main(String[] args) {
		Animal1 myObj = new Animal1();
		
		myObj.displayInfo();
		myObj.printSound();

	}
}
