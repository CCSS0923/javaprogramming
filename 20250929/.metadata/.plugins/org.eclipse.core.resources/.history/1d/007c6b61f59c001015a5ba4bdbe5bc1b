package abstracttest;

class Parent {
	public void show() { System.out.println("parent show( )"); }
}

public class Main {
	
	public static void main(String[] args) 	{

		Child1 c1 = new Child1( );
		c1.show( );
		c1.print();
		
		System.out.println();
		Child2 c2 = new Child2( );
		c2.show( );
		c2.print();
		
		System.out.println();
		System.out.println("**** *****");
		Parent x = new Child1( );
		Parent y = new Child2( );
		x.show( );
		y.show( );
		x.print( );
		y.print( );
	}
}