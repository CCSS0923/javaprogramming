import java.util.Scanner;

public class If03 {

	public static void main(String[] args) {
		System.out.println("학점을 입력하세요.");
		
		Scanner s = new Scanner(System.in);
		int score = s.nextInt();
		
		if(score >= 90) {
			System.out.println("A학점");
		}
		else if(score >= 80) {
			System.out.println("B학점");
		}

}	